# Agent Marketplace Plugin Configuration

# API Configuration
MARKETPLACE_API_URL=http://localhost:3000
# Production: https://agent-marketplace-api.example.com

# Agent Identity
MARKETPLACE_AGENT_ID=agent-unique-id-here
# Example: agent-babo, agent-alice, etc.

# Blockchain Configuration (Optional - for direct contract interaction)
MARKETPLACE_WALLET_ADDRESS=0x0000000000000000000000000000000000000000
MARKETPLACE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
MARKETPLACE_PROVIDER_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_KEY_HERE

# Feature Flags (Optional)
MARKETPLACE_AUTO_APPROVE_LISTINGS=false
MARKETPLACE_AUTO_APPROVE_PURCHASES=false
MARKETPLACE_ENABLE_NOTIFICATIONS=true

# Logging (Optional)
LOG_LEVEL=info
# Options: debug, info, warn, error
